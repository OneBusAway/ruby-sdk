    attr_reader api_key: String

    attr_reader agencies_with_coverage: Onebusaway::Resources::AgenciesWithCoverage

    attr_reader agency: Onebusaway::Resources::Agency

    attr_reader vehicles_for_agency: Onebusaway::Resources::VehiclesForAgency

    attr_reader config: Onebusaway::Resources::Config

    attr_reader current_time: Onebusaway::Resources::CurrentTime

    attr_reader stops_for_location: Onebusaway::Resources::StopsForLocation

    attr_reader stops_for_route: Onebusaway::Resources::StopsForRoute

    attr_reader stops_for_agency: Onebusaway::Resources::StopsForAgency

    attr_reader stop: Onebusaway::Resources::Stop

    attr_reader stop_ids_for_agency: Onebusaway::Resources::StopIDsForAgency

    attr_reader schedule_for_stop: Onebusaway::Resources::ScheduleForStop

    attr_reader route: Onebusaway::Resources::Route

    attr_reader route_ids_for_agency: Onebusaway::Resources::RouteIDsForAgency

