    attr_reader trips_for_route: Onebusaway::Resources::TripsForRoute

    attr_reader report_problem_with_stop: Onebusaway::Resources::ReportProblemWithStop

    attr_reader report_problem_with_trip: Onebusaway::Resources::ReportProblemWithTrip

    attr_reader search_for_stop: Onebusaway::Resources::SearchForStop

    attr_reader search_for_route: Onebusaway::Resources::SearchForRoute

    attr_reader block: Onebusaway::Resources::Block

    attr_reader shape: Onebusaway::Resources::Shape

    private def auth_headers: -> Hash[String, String]

    def initialize: (
      base_url: String?,
      api_key: String?,
      max_retries: Integer,
      timeout: Float,
      initial_retry_delay: Float,
      max_retry_delay: Float
