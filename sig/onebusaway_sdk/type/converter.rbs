module OnebusawaySDK
  module Type
    module Converter
      type input = OnebusawaySDK::Type::Converter | Class

      type state =
        {
          strictness: bool | :strong,
          exactness: { yes: Integer, no: Integer, maybe: Integer },
          branched: Integer
        }

      def coerce: (
        top value,
        state: OnebusawaySDK::Type::Converter::state
      ) -> top

      def dump: (top value) -> top

      def self.type_info: (
        {
          const: (nil | bool | Integer | Float | Symbol)?,
          enum: ^-> OnebusawaySDK::Type::Converter::input?,
          union: ^-> OnebusawaySDK::Type::Converter::input?
        }
        | ^-> OnebusawaySDK::Type::Converter::input
        | OnebusawaySDK::Type::Converter::input spec
      ) -> (^-> top)

      def self.coerce: (
        OnebusawaySDK::Type::Converter::input target,
        top value,
        ?state: OnebusawaySDK::Type::Converter::state
      ) -> top

      def self.dump: (
        OnebusawaySDK::Type::Converter::input target,
        top value
      ) -> top
    end
  end
end
